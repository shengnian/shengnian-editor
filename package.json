{
  "name": "shengnian-editor",
  "version": "1.2.0",
  "description": "Shengnian React Rich Text Editor ",
  "main": "dist/shengnian-editor.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm run build-lib && npm run build-dist",
    "build-dist": "rimraf dist && webpack",
    "build-lib": "rimraf lib && babel src --ignore \"_*\" --out-dir lib --copy-files",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "flow",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --content-base assets/",
    "test": "npm run lint && npm run typecheck && npm run test-src",
    "test-src": "mocha \"src/**/__tests__/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git@shengnian.org:/shengnian/shengnian-editor.git"
  },
  "keywords": [
    "React",
    "Draft.js",
    "WYSIWYG",
    "Editor"
  ],
  "author": "c@shengnian.org",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "eslint": "^4.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^21.2.1",
    "flow-bin": "^0.56.0",
    "mocha": "^4.0.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "class-autobind": "^0.1.4",
    "classnames": "^2.2.5",
    "draft-js": "^0.10.3",
    "draft-js-export-html": "^1.2.0",
    "draft-js-export-markdown": "^1.2.0",
    "draft-js-import-html": "^1.2.0",
    "draft-js-import-markdown": "^1.2.0",
    "draft-js-prism": "^1.0.5",
    "draft-js-utils": "^1.2.0",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "prismjs": "^1.8.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
